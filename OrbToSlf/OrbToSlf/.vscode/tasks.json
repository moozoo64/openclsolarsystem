{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build Debug",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/OrbToSlf.csproj"
            ],
            "problemMatcher": "$msCompile",
            "group": "build"
        },
        {
            "label": "build Release",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/OrbToSlf.csproj",
                "-c",
                "Release"
            ],
            "problemMatcher": "$msCompile",
            "group": "build"
        },
        {
            "label": "clean Debug",
            "command": "dotnet",
            "type": "process",
            "args": [
                "clean",
                "${workspaceFolder}/OrbToSlf.csproj"
            ],
            "problemMatcher": "$msCompile",
        },
        {
            "label": "clean Release",
            "command": "dotnet",
            "type": "process",
            "args": [
                "clean",
                "${workspaceFolder}/OrbToSlf.csproj",
                "-c",
                "Release"
            ],
            "problemMatcher": "$msCompile",
        }
    ]
}